{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            },
            "Using": [
                "Serilog.Sinks.OpenTelemetry"
            ]
        },
        "WriteTo": [
            {
                "Name": "OpenTelemetry",
                "Args": {
                    "Endpoint": "http://localhost:4317",
                    "Protocol": "Grpc",
                    "ResourceAttributes": {
                        "service.name": "Activity_API"
                    }
                }
            }
        ]
    },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStringSql": "Server=localhost;Database=WorkerDb;User ID=SA;Password=Change123!@#;TrustServerCertificate=True;",

  "MessageQueueConnection": "host=localhost;username=guest;password=guest",
   "OpenTelemetryURL":"http://localhost:4317",
  "NameApp": "Worker"
}
